# Categorias de recargas exitosas - TOTAL
application.web.recharge.report.query.RecargasExitosasTotalOpcion=select   COUNT(a.gsm) as CANTIDAD from     recarga3g.web_rech_transaction a, tbl_rechargeoption b, tbl_rechoption_nonvouchrech r where    a.ERROR_CODE = '0'   and    a.AMOUNT_CHARGE>0 and    a.rechvalue = r.rechvalue  and    r.RECHOPTION = b.RECHOPTION and    DATE_TIME between to_date(to_char(sysdate-1,'yyyymmdd') || ' 00:00:00', 'yyyymmdd hh24:mi:ss') and to_date(to_char(sysdate-1,'yyyymmdd') || ' 23:59:59', 'yyyymmdd hh24:mi:ss') and    RT_DATA_PARTITION_KEY between to_date(to_char(sysdate-1,'yyyymmdd') || ' 00:00:00', 'yyyymmdd hh24:mi:ss') and to_date(to_char(sysdate-1,'yyyymmdd') || ' 23:59:59', 'yyyymmdd hh24:mi:ss') and AMOUNT_CHARGE in (select FACE_VALUE from card_category) and ID_INSTITUTION='910'

# Categorias de recargas fallidas - TOTAL
application.web.recharge.report.query.RecargasFallidasTotalOpcion=select COUNT(a.gsm) as CANTIDAD from     recarga3g.web_rech_transaction a, tbl_rechargeoption b, tbl_rechoption_nonvouchrech r where a.rechvalue = r.rechvalue and    r.RECHOPTION = b.RECHOPTION AND ERROR_CODE <> '0' and DATE_TIME between to_date(to_char(sysdate-1,'yyyymmdd') || ' 00:00:00', 'yyyymmdd hh24:mi:ss') and to_date(to_char(sysdate-1,'yyyymmdd') || ' 23:59:59', 'yyyymmdd hh24:mi:ss') and RT_DATA_PARTITION_KEY between to_date(to_char(sysdate-1,'yyyymmdd') || ' 00:00:00', 'yyyymmdd hh24:mi:ss') and to_date(to_char(sysdate-1,'yyyymmdd') || ' 23:59:59', 'yyyymmdd hh24:mi:ss') and ID_INSTITUTION='910'

# Recargas por canales - Subtotal
application.web.recharge.report.query.RecargasExitosasTotalPorCanalOpcion=select  COUNT(gsm) as CANTIDAD from    web_rech_transaction a, tbl_rechargeoption b, tbl_rechoption_nonvouchrech r where   a.channel=? and   a.AMOUNT_CHARGE>0 and   a.error_code = '0' and   a.rechvalue = r.rechvalue and   r.RECHOPTION = b.RECHOPTION and   DATE_TIME between to_date(to_char(sysdate-1,'yyyymmdd') || ' 00:00:00', 'yyyymmdd hh24:mi:ss') and to_date(to_char(sysdate-1,'yyyymmdd') || ' 23:59:59', 'yyyymmdd hh24:mi:ss') and   RT_DATA_PARTITION_KEY between to_date(to_char(sysdate-1,'yyyymmdd') || ' 00:00:00', 'yyyymmdd hh24:mi:ss') and to_date(to_char(sysdate-1,'yyyymmdd') || ' 23:59:59', 'yyyymmdd hh24:mi:ss') and   a.AMOUNT_CHARGE in (select FACE_VALUE from card_category) and ID_INSTITUTION='910'

#
application.web.recharge.report.query.RecargasFallidasTotalPorCanalOpcion=select  COUNT(gsm) as CANTIDAD from    web_rech_transaction a, tbl_rechargeoption b, tbl_rechoption_nonvouchrech r where   a.channel=? and   a.error_code <> '0' and   a.rechvalue = r.rechvalue and   r.RECHOPTION = b.RECHOPTION and   DATE_TIME between to_date(to_char(sysdate-1,'yyyymmdd') || ' 00:00:00', 'yyyymmdd hh24:mi:ss') and to_date(to_char(sysdate-1,'yyyymmdd') || ' 23:59:59', 'yyyymmdd hh24:mi:ss') and   RT_DATA_PARTITION_KEY between to_date(to_char(sysdate-1,'yyyymmdd') || ' 00:00:00', 'yyyymmdd hh24:mi:ss') and  to_date(to_char(sysdate-1,'yyyymmdd') || ' 23:59:59', 'yyyymmdd hh24:mi:ss') and   a.AMOUNT_CHARGE in (select FACE_VALUE from card_category) and ID_INSTITUTION='910'

#
application.web.recharge.report.query.Canales=select count(gsm), a.channel as CANAL from web_rech_transaction a where DATE_TIME between to_date(to_char(sysdate-1,'yyyymmdd') || ' 00:00:00', 'yyyymmdd hh24:mi:ss') and to_date(to_char(sysdate-1,'yyyymmdd') || ' 23:59:59', 'yyyymmdd hh24:mi:ss')  and RT_DATA_PARTITION_KEY between to_date(to_char(sysdate-1,'yyyymmdd') || ' 00:00:00', 'yyyymmdd hh24:mi:ss') and to_date(to_char(sysdate-1,'yyyymmdd') || ' 23:59:59', 'yyyymmdd hh24:mi:ss') and ID_INSTITUTION='910' group by a.channel

#
