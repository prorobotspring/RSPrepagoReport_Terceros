application.web.recharge.report.query.RecargasFallidasGSMPorCanal=select COUNT(distinct(gsm)) as CANTIDAD from recarga3g.web_rech_transaction where CHANNEL=? and ERROR_CODE <> '0' and DATE_TIME between to_date(to_char(sysdate-1,'yyyymmdd') || ' 00:00:00', 'yyyymmdd hh24:mi:ss') and to_date(to_char(sysdate-1,'yyyymmdd') || ' 23:59:59', 'yyyymmdd hh24:mi:ss') and RT_DATA_PARTITION_KEY between to_date(to_char(sysdate-1,'yyyymmdd') || ' 00:00:00', 'yyyymmdd hh24:mi:ss') and to_date(to_char(sysdate-1,'yyyymmdd') || ' 23:59:59', 'yyyymmdd hh24:mi:ss') and ID_INSTITUTION='910'

