application.web.recharge.report.query.CategoriaRecargasExitosasOpcion=select count(a.gsm), b.DESCRIPTIONRECHOPTION from web_rech_transaction a, tbl_rechargeoption b, tbl_rechoption_nonvouchrech r where a.channel=? and a.AMOUNT_CHARGE>0 and a.error_code = '0' and a.rechvalue = r.rechvalue and r.RECHOPTION = b.RECHOPTION and DATE_TIME between to_date (to_char(sysdate-1,'yyyymmdd') || ' 00:00:00', 'yyyymmdd hh24:mi:ss') and     to_date(to_char(sysdate-1,'yyyymmdd') || ' 23:59:59', 'yyyymmdd hh24:mi:ss') and     RT_DATA_PARTITION_KEY between to_date(to_char(sysdate-1,'yyyymmdd') || ' 00:00:00', 'yyyymmdd hh24:mi:ss') and     to_date(to_char(sysdate-1,'yyyymmdd') || ' 23:59:59', 'yyyymmdd hh24:mi:ss') and ID_INSTITUTION='910' and     a.AMOUNT_CHARGE in      (select FACE_VALUE from card_category) group by b.RECHOPTION, b.DESCRIPTIONRECHOPTION order by b.RECHOPTION

